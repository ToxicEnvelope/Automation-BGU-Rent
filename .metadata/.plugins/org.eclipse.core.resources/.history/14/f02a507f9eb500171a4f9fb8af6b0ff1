package il.co.bgurent.apis;


public class SearchAPI extends AbstractAPIs {

	private int _rooms;
	private int _rent;
	private int _secId;
	
	
	// GETTERS
	protected int getRooms() {
		return _rooms;
	}
	protected int getRent() {
		return _rent;
	}
	protected int getSectionID() {
		return _secId;
	}
	// SETTERS
	protected void setRooms(int rms) {
		if(rms >= 0) {
			this._rooms = rms;
		}
	}
	protected void setRent(int rnt) {
		if(rnt > 0) {
			this._rent = rnt;
		}
	}
	protected void setSectionID(int secId) {
		if(secId >= 0 && secId <=7) {
			this._secId = secId;
		}
	}
}
