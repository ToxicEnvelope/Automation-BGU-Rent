public class BaseTest {

	
	static WebDriver driver;
	final String DATA = "/src/il/co/bgurent/or";
	final String LOC = "/";
	static final UIMap dataMap = new UIMap( System.setProperty(System.getProperty("user.dir"), ) );
	static final UIMap locMap = new UIMap(  );
	

	@BeforeClass
	public void startUp() {
		System.setProperty();
	}
	
	@AfterClass
	public void tearDown() {
