package il.co.bgurent.apis;

import org.apache.http.HttpRequest;

public abstract class AbstractAPIs implements HttpRequest {
	
	String _rent;
	String _rooms;
	int    _sectionId;
	
	public AbstractAPIs() {
		
	}
	
	
	// GETTERS 
	protected String getRent() {
		return _rent;
	}
	protected String getRooms() {
		return _rooms;
	}
	protected int getSectionID() {
		return _sectionId;
	}
	// SETTERS
	protected void setRent(String rnt) {
		this._rent = rnt;
	}
	protected void setRooms(String rms) {
		this._rooms = rms;
	}
	protected void setSectionID(int secId) {
		this._sectionId = secId;
	}
	// IMPLEMENTED METHODS
}
