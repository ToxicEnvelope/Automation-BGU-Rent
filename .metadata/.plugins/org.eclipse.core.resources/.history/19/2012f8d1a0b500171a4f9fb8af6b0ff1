package il.co.bgurent.apis;


public class SearchAPI extends AbstractAPIs {

	// FIELDS
	private int _rooms;
	private int _rent;
	private int _secId;
	
	
	// CONSTRUCTOR
	public SearchAPI(String uri) {
		super(uri);
	}
	
	// GETTERS
	protected int getRooms() {
		return _rooms;
	}
	protected int getRent() {
		return _rent;
	}
	protected int getSectionID() {
		return _secId;
	}
	// SETTERS
	protected void setRooms(int rms) {
		try {
			if(rms >= 0) {
				this._rooms = rms;
			}
		}
		catch (Exception e) {
			e.printStackTrace();
		}
	}
	protected void setRent(int rnt) {
		try {
			if(rnt >= 0) {
				this._rent = rnt;
			}
		}
		catch (Exception e) {
			e.printStackTrace();
		}
	}
	protected void setSectionID(int secId) {
		try {
			if(secId >= 0 && secId <= 7) {
				this._secId = secId;
			}
		}
		catch (Exception e) {
			e.printStackTrace();
		}	}
	// INHERITED METHODS
	protected void sendGET() {}
	protected void sendPOST() {}

}
