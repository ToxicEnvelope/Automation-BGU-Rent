package il.co.bgurent.apis;

import com.google.gson.JsonObject;



public abstract class AbstractAPIs {
	
	protected final String USER_AGENT = "Mozilla/5.0";
	
	protected String getUserAgent() {
		return USER_AGENT;
	}
	
	// HTTP GET request
	protected abstract void sendGet(String uri, String token, String contry);
	// HTTP POST request
	protected abstract void sendPost(String uri, String token, JsonObject data);
		
}
