package il.co.bgurent.apis;


public abstract class AbstractAPIs {
	
	private String _uri = "";
	private String _userAgent = "Mozilla/5.0";
	
	
	// GETTERS
	protected String getUserAgent() {
		return _userAgent;
	}
	protected String getUri() {
		return _uri;
	}
	// SETTERS
	protected void setUri(String uri) {
		if(uri != null)
			this._uri = uri;
	}
	
}
