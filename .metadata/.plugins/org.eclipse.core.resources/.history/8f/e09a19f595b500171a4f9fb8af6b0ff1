package il.co.bgurent.apis;

import com.google.gson.JsonObject;



public abstract class AbstractAPIs {
	
	protected final String USER_AGENT = "Mozilla/5.0";
	
	// GETTERS
	protected String getUserAgent() {
		return USER_AGENT;
	}
	
	// HTTP GET request
	protected abstract void sendSearchAPIGet(String uri,int rooms, int rent, int sectionId);
	// HTTP POST request
	protected abstract void sendSearchAPIPost(String uri, int, JsonObject data);
		
}
