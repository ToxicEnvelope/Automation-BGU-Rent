package il.co.bgurent.apis;

public abstract class AbstractAPIs {
	
	String _rent;
	String _rooms;
	int    _sectionId;
	
	public AbstractAPIs() {
		
	}
	
	
	// GETTERS 
	protected String getRent() {
		return _rent;
	}
	
	protected String getRooms() {
		return _rooms;
	}
	
	protected int getSectionID() {
		return _sectionId;
	}
	// SETTERS
	protected void setRent(String rnt) {
		this._rent = rnt;
	}
	
}
